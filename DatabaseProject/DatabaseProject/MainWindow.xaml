                <ComboBoxItem Content="Cash"/>
                <ComboBoxItem Content="Credit"/>
                <ComboBoxItem Content="Debit"/>
            </ComboBox>
            <ComboBox x:Name="combo_trans_type" HorizontalAlignment="Left" Height="23" Margin="10,316,0,0" VerticalAlignment="Top" Width="189" MaxDropDownHeight="50" SelectionChanged="combo_trans_type_SelectionChanged">
                <ComboBoxItem Content="Ticket Sale"/>
                <ComboBoxItem Content="Item Sale"/>
            </ComboBox>
            <DataGrid x:Name="datatable_transactions" HorizontalAlignment="Left" Height="344" Margin="214,10,0,0" VerticalAlignment="Top" Width="566" IsReadOnly="True"/>

            <Label x:Name="ItemLabel" Content="Item" Visibility="Collapsed" HorizontalAlignment="Left" VerticalAlignment="Top" Width="127" Height="27" RenderTransformOrigin="-0.589,1.908" Margin="10,339,0,0"/>
