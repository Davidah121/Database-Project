            <DataGrid x:Name="datagrid_emp" HorizontalAlignment="Left" Height="265" Margin="382,142,0,0" VerticalAlignment="Top" Width="375" IsReadOnly="True"/>
            <TextBox x:Name="emp_field_fname" HorizontalAlignment="Left" Height="29" Margin="155,217,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="199" Background="#FFDF7878"/>
